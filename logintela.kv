            pos: self.pos

    LogoMeioTurbinaGram:
        source: 'recursos/LOGO_TURBINA_GRAMLicone.png'
        size_hint_y: .14
        pos_hint: {'center_x': .5, 'center_y': .66}
    
    ButTest:
        on_release: app.root.current = 'loading'
        #on_press: root.logar()
        
    #BotaoLoginInsta:
        #size_hint: .70, .08
